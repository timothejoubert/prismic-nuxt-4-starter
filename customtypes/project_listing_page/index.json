{
	"format": "page",
	"id": "project_listing_page",
	"label": "Project listing page",
	"repeatable": false,
	"status": true,
	"json": {
		"Main": {
			"title": {
				"type": "Text",
				"config": {
					"label": "Title",
					"placeholder": ""
				}
			},
			"content": {
				"type": "StructuredText",
				"config": {
					"label": "Content",
					"placeholder": "",
					"allowTargetBlank": true,
					"multi": "paragraph,strong,em,hyperlink"
				}
			},
			"image": {
				"type": "Image",
				"config": {
					"label": "Image",
					"constraint": {},
					"thumbnails": []
				}
			},
			"slices": {
				"type": "Slices",
				"fieldset": "Slice Zone",
				"config": {
					"choices": {
						"simple_text_slice": {
							"type": "SharedSlice"
						},
						"contact_slice": {
							"type": "SharedSlice"
						}
					}
				}
			}
		},
		"SEO & Metadata": {
			"meta_title": {
				"config": {
					"label": "Meta Title",
					"placeholder": "A title of the page used for social media and search engines"
				},
				"type": "Text"
			},
			"meta_description": {
				"config": {
					"label": "Meta Description",
					"placeholder": "A brief summary of the page"
				},
				"type": "Text"
			},
			"meta_image": {
				"config": {
					"constraint": {
						"height": 1260,
						"width": 2400
					},
					"label": "Meta Image",
					"thumbnails": []
				},
				"type": "Image"
			}
		}
	}
}
